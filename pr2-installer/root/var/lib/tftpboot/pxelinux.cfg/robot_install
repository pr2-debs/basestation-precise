default menu.c32
prompt 0
timeout 10
ontimeout robot_precise64

menu title Pr2 Installer

label robot_precise64
    menu label robot_precise64 : wipe hard drive and install 64-bit precise for robot
    kernel linux64
    append initrd=initrd64.gz auto url=http://10.68.0.6/robot_precise64.preseed locale=en_US.UTF-8 keyboard-configuration/layoutcode=us interface=eth1 netcfg/disable_autoconfig=true netcfg/get_hostname=c1 netcfg/get_domain=willowgarage.com netcfg/get_nameservers=10.68.0.6 netcfg/get_ipaddress=10.68.0.1 netcfg/get_netmask=255.255.255.0 netcfg/get_gateway=10.68.0.6 root=/dev/rd/0 numa=noacpi anna/no_kernel_modules=true --
