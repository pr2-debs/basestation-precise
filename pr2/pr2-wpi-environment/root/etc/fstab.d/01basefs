tmpfs                   /tmp            tmpfs   defaults                        0       0
proc                    /proc           proc    nodev,noexec,nosuid             0       0
/dev/mapper/vg00-root   /               ext4    barrier=0,errors=remount-ro     0       1
/dev/sda1               /boot           ext2    defaults                        0       2
/dev/mapper/vg00-hwlog  /hwlog          ext4    barrier=0,defaults              0       2
